:{"line":42,"column":200,"offset":2192},"indent":[]}}],"position":{"start":{"line":42,"column":3,"offset":1995},"end":{"line":42,"column":200,"offset":2192},"indent":[]}}],"position":{"start":{"line":42,"column":1,"offset":1993},"end":{"line":42,"column":200,"offset":2192},"indent":[]}},{"type":"listItem","loose":false,"checked":null,"children":[{"type":"paragraph","children":[{"type":"strong","children":[{"type":"text","value":"Reverse Cache","position":{"start":{"line":43,"column":5,"offset":2197},"end":{"line":43,"column":18,"offset":2210},"indent":[]}}],"position":{"start":{"line":43,"column":3,"offset":2195},"end":{"line":43,"column":20,"offset":2212},"indent":[]}},{"type":"text","value":": Your server infrastructure can implement its own cache for backend services. This way, content can be served from the point-of-contact instead of hitting backend servers on each request.","position":{"start":{"line":43,"column":20,"offset":2212},"end":{"line":43,"column":208,"offset":2400},"indent":[]}}],"position":{"start":{"line":43,"column":3,"offset":2195},"end":{"line":43,"column":208,"offset":2400},"indent":[]}}],"position":{"start":{"line":43,"column":1,"offset":2193},"end":{"line":43,"column":208,"offset":2400},"indent":[]}}],"position":{"start":{"line":41,"column":1,"offset":1838},"end":{"line":43,"column":208,"offset":2400},"indent":[1,1]}},{"type":"heading","depth":3,"children":[{"type":"text","value":"How Does the Browser Know What to Cache?","position":{"start":{"line":45,"column":5,"offset":2406},"end":{"line":45,"column":45,"offset":2446},"indent":[]}}],"position":{"start":{"line":45,"column":1,"offset":2402},"end":{"line":45,"column":45,"offset":2446},"indent":[]}},{"type":"paragraph","children":[{"type":"text","value":"There are two modern HTTP response headers that define how a resource should be cached: Cache-Control and ETag.","position":{"start":{"line":46,"column":1,"offset":2447},"end":{"line":46,"column":112,"offset":2558},"indent":[]}}],"position":{"start":{"line":46,"column":1,"offset":2447},"end":{"line":46,"column":112,"offset":2558},"indent":[]}},{"type":"heading","depth":4,"children":[{"type":"text","value":"Cache-Control","position":{"start":{"line":48,"column":6,"offset":2565},"end":{"line":48,"column":19,"offset":2578},"indent":[]}}],"position":{"start":{"line":48,"column":1,"offset":2560},"end":{"line":48,"column":19,"offset":2578},"indent":[]}},{"type":"paragraph","children":[{"type":"text","value":"When your browser requests resources from a server the first time, it stores the returning resources in its cache according to this header. when your browser needs to request that resource again, it will check its cache to see if the resource is there and that it still fits the Cache-Control specs.","position":{"start":{"line":50,"column":1,"offset":2580},"end":{"line":50,"column":300,"offset":2879},"indent":[]}}],"position":{"start":{"line":50,"column":1,"offset":2580},"end":{"line":50,"column":300,"offset":2879},"indent":[]}},{"type":"paragraph","children":[{"type":"text","value":"The Cache-Control header has a couple different parameters that can be set from the server:","position":{"start":{"line":52,"column":1,"offset":2881},"end":{"line":52,"column":92,"offset":2972},"indent":[]}}],"position":{"start":{"line":52,"column":1,"offset":2881},"end":{"line":52,"column":92,"offset":2972},"indent":[]}},{"type":"heading","depth":5,"children":[{"type":"text","value":"no-cache or no-store","position":{"start":{"line":54,"column":7,"offset":2980},"end":{"line":54,"column":27,"offset":3000},"indent":[]}}],"position":{"start":{"line":54,"column":1,"offset":2974},"end":{"line":54,"column":27,"offset":3000},"indent":[]}},{"type":"paragraph","children":[{"type":"html","value":"<code class=\"language-text\">no-cache</code>","position":{"start":{"line":56,"column":1,"offset":3002},"end":{"line":56,"column":11,"offset":3012},"indent":[]}},{"type":"text","value":": This resource should always be requested from the server instead of automatically loaded from cache. However, the protocol about ETags still applies, and if the ETags for the client version and the server version match up then the server will instruct the client to use its cached version.","position":{"start":{"line":56,"column":11,"offset":3012},"end":{"line":56,"column":302,"offset":3303},"indent":[]}}],"position":{"start":{"line":56,"column":1,"offset":3002},"end":{"line":56,"column":302,"offset":3303},"indent":[]}},{"type":"paragraph","children":[{"type":"html","value":"<code class=\"language-text\">no-store</code>","position":{"start":{"line":58,"column":1,"offset":3305},"end":{"line":58,"column":11,"offset":3315},"indent":[]}},{"type":"text","value":": is simpler in that it tells the browser to always request the resource from the server without checking ETags.","position":{"start":{"line":58,"column":11,"offset":3315},"end":{"line":58,"column":123,"offset":3427},"indent":[]}}],"position":{"start":{"line":58,"column":1,"offset":3305},"end":{"line":58,"column":123,"offset":3427},"indent":[]}},{"type":"heading","depth":5,"children":[{"type":"text","value":"public or private","position":{"start":{"line":60,"column":7,"offset":3435},"end":{"line":60,"column":24,"offset":3452},"indent":[]}}],"position":{"start":{"line":60,"column":1,"offset":3429},"end":{"line":60,"column":24,"offset":3452},"indent":[]}},{"type":"paragraph","children":[{"type":"html","value":"<code class=\"language-text\">public</code>","position":{"start":{"line":62,"column":1,"offset":3454},"end":{"line":62,"column":9,"offset":3462},"indent":[]}},{"type":"text","value":" means that a resource can be cached by anyone – but this usually isn’t necessary as by default, defining the max-age part of the header will also set it as public.","position":{"start":{"line":62,"column":9,"offset":3462},"end":{"line":62,"column":173,"offset":3626},"indent":[]}}],"position":{"start":{"line":62,"column":1,"offset":3454},"end":{"line":62,"column":173,"offset":3626},"indent":[]}},{"type":"paragraph","children":[{"type":"html","value":"<code class=\"language-text\">private</code>","position":{"start":{"line":64,"column":1,"offset":3628},"end":{"line":64,"column":10,"offset":3637},"indent":[]}},{"type":"text","value":" means that only the user’s browser can cache the resource, and not any intermediaries such as a CDN. This is especially important when dealing with resources with personal information, such as when you log into your bank’s website.","position":{"start":{"line":64,"column":10,"offset":3637},"end":{"line":64,"column":242,"offset":3869},"indent":[]}}],"position":{"start":{"line":64,"column":1,"offset":3628},"end":{"line":64,"column":242,"offset":3869},"indent":[]}},{"type":"heading","depth":5,"children":[{"type":"text","value":"max-age","position":{"start":{"line":66,"column":7,"offset":3877},"end":{"line":66,"column":14,"offset":3884},"indent":[]}}],"position":{"start":{"line":66,"column":1,"offset":3871},"end":{"line":66,"column":14,"offset":3884},"indent":[]}},{"type":"paragraph","children":[{"type":"text","value":"Finally, ","position":{"start":{"line":68,"column":1,"offset":3886},"end":{"line":68,"column":10,"offset":3895},"indent":[]}},{"type":"html","value":"<code class=\"language-text\">max-age</code>","position":{"start":{"line":68,"column":10,"offset":3895},"end":{"line":68,"column":19,"offset":3904},"indent":[]}},{"type":"text","value":" determines the length of time in seconds that this resource should be cached. max-age=120 means that this resource can be cached and reused for 2 minutes.","position":{"start":{"line":68,"column":19,"offset":3904},"end":{"line":68,"column":174,"offset":4059},"indent":[]}}],"position":{"start":{"line":68,"column":1,"offset":3886},"end":{"line":68,"column":174,"offset":4059},"indent":[]}},{"type":"heading","depth":4,"children":[{"type":"text","value":"ETag","position":{"start":{"line":70,"column":6,"offset":4066},"end":{"line":70,"column":10,"offset":4070},"indent":[]}}],"positi